#version 330 coreout vec4 fragColor;in vec2 v_TexCoords;uniform sampler2D texture_diffuse1;uniform sampler2D texture_specular1;uniform sampler2D texture_normal1;void main(){    vec4 diffuse = texture(texture_diffuse1, v_TexCoords);    vec4 spcular = texture(texture_diffuse1, v_TexCoords);    vec4 normal = texture(texture_diffuse1, v_TexCoords);    vec4 result = diffuse + specular + normal;    fragColor = result;}